package day_1_hcl;

public class ExpOnIdentityMatrix {
	
	public static void main(String[] args) {
		int x[][] = {{1,0,0}, {0,1,0}, {0,0,1}};
		
		for(int temp[]: x) {
			for(int val: temp) {
				System.out.print(val);
			}
			System.out.println();
		}
		
//		for(int i=0;i<x.length;i++) {
//			for(int j=0;j<x[i].length;j++) {
//				System.out.print(x[i][j]);
//			}
//			System.out.println();
//		}
	}

}
