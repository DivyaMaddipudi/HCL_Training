		int arr[][] = new int[4][4];
		int arr2[][] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
		
		int newArr[][] = new int[]][];
		
		for(int i=0;i<arr.length;i++) {
			for(int j=0;j<arr[i].length;j++) {
				int newArr[]
				newArr = arr[i][j] + arr2[i][j];
			}
			System.out.println();
		}
	}
	

}
