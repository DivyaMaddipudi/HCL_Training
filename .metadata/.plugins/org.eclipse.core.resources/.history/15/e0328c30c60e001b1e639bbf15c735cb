package lab_assignment_1;

public class Lab1_Q2 {
	
	public static void main(String[] args) {
		
		
		for(int i=1;i<=9;i++) {
			System.out.println("* i");
			System.out.println("--------------------");
			for(int j=1;j<=9;j++) {
				System.out.print(i*j + " ");
			}
			System.out.println();
		}
		
	}

}
