package day_1_hcl;

import java.util.Scanner;

public class DemoArray {
	
	public static void main(String[] args) {
		
		int x[] = new int[5];
		
		int sum = 0;
		for(int i=0;i<x.length;i++) {
			Scanner sc = new Scanner(System.in);
			x[i] = sc.nextInt();
			sum += x[i]
			System.out.println(x[i]);
		}
	}

}
